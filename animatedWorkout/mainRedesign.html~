<script>
var check=0;
$(document).ready(function(){
	//$( ".cellExercise" ).not( ":even" ).css( "background-color", "#f5f5f5" );
	//Group Checkboxes
	$("#toggleCheck").click(function(){
			if (check == 0){
				$(":checkbox").prop("checked", true);
				$(".cellExercise").removeClass("incomplete").addClass("complete");
				check=1;
				//fix notes section when inverted to a darker color #f5f5f5


	}
	else{
				$(":checkbox").prop("checked", false);
				$(".cellExercise").removeClass("complete").addClass("incomplete");
				check=0;
								//add transition to exercise plate
										}
									});
	//Indivisual checkboxes
$('input[type="checkbox"]').click(function() {
    if (this.checked) {
    	$(this).parent().parent().removeClass("incomplete").addClass("complete");
    	    }
        else  {
    	$(this).parent().parent().removeClass("complete").addClass("incomplete");
    	    }
 							});

$(".container").fadeIn("slow");

});
</script>


